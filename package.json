{
  "name": "y-note",
  "version": "1.0.0",
  "description": "my technology note under yuque",
  "main": "index.js",
  "private": true,
  "author": "Nooldey",
  "license": "ISC",
  "keywords": [
    "yuque",
    "sync",
    "hexo"
  ],
  "scripts": {
    "sync": "yuque-hexo sync",
    "clean:yuque": "yuque-hexo clean",
    "build": "hexo g",
    "deploy": "npm run sync && hexo g -d"
  },
  "yuqueConfig": {
    "adapter": "hexo",
    "baseUrl": "https://www.yuque.com/api/v2",
    "concurrency": 5,
    "onlyPublished": true,
    "login": "nooldey",
    "mdNameFormat": "slug",
    "postPath": "source/_posts/gists",
    "repo": "gists"
  },
  "devDependencies": {
    "yuque-hexo": "^1.4.2",
    "hexo-server": "^0.3.3"
  },
  "hexo": {
    "version": "3.8.0"
  },
  "dependencies": {
    "hexo": "^3.8.0",
    "hexo-generator-archive": "^0.1.5",
    "hexo-generator-category": "^0.1.3",
    "hexo-generator-index": "^0.2.1",
    "hexo-generator-search": "^2.4.0",
    "hexo-generator-tag": "^0.2.0",
    "hexo-renderer-ejs": "^0.3.1",
    "hexo-renderer-less": "^1.0.0",
    "hexo-renderer-marked": "^0.3.2",
    "hexo-deployer-git": "^1.0.0"
  }
}
